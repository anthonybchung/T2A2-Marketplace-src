<% excursions.each do |excursion| %>
<p>
<%= excursion.name %>
<%= excursion.description %>
<%= excursion.arrive_time %>
<%= excursion.street_number %>
<%= excursion.street_name %>
<%= excursion.suburb %>
<%= excursion.state %>
<%= excursion.post_code %>
</p>
<%= link_to "edit details", edit_excursion_path(excursion.id), class: "btn btn-primary" %>

<% end %>