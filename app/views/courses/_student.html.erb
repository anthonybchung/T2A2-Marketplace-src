<% #make a student card to display the following %>
<% # user_name, email, banned? %>

<% enrollments.each do |enrollment| %>
<!-- style row colour, if user is banned, highlight row red.-->
<%if enrollment.banned%>
<tr class="table-danger">
<%else%>
<tr>
<% end %>
    <td><%= enrollment.user.user_name %></td>
    <td><%= enrollment.user.email %></td>
    <td><%= link_to "edit details", edit_enrolled_path(enrollment.id) %></td>
</tr>
<% end %>




